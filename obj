    $('#guidelinesCheckbox'+1055298).on('change', function(){
        if ($('#guidelinesCheckbox'+1055298).attr('checked') == 'checked')
            $('#guidelinesButton'+1055298).removeClass('disabled');
        else
            $('#guidelinesButton'+1055298).addClass('disabled');
    });

