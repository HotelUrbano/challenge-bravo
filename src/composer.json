{
    "name": "carlosvinicius/currency-converter",
    "description": "Currency Converter API",
    "keywords": ["currency","conversion"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Vinicius",
            "email": "me@carlosvinicius.com.br"
        }
    ],
    "require": {
        "php": "7.2.17",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/redis": "^5.8",
        "laravel/lumen-framework": "5.8.*",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.9",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Hurb\\Api\\": "api/app/",
            "Hurb\\CurrencyConverter\\": "converter/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hurb\\Tests\\CurrencyConverter\\": "tests/converter/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
